{
  "expo": {
    "name": "DubsWay Video AI",
    "slug": "dubsway-video-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dubsway.videoai",
      "buildNumber": "1",
      "infoPlist": {
        "NSDocumentPickerUsageDescription": "This app uses the document picker to select video files for upload.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select videos for upload.",
        "NSCameraUsageDescription": "This app needs access to your camera to record videos.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record videos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366F1"
      },
      "package": "com.dubsway.videoai",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-av"
    ],
    "extra": {
      "eas": {
        "projectId": "0115cafd-784d-40ba-9ca4-aac357e94f4e"
      }
    },
    "owner": "ashish1991.2009"
  }
}
